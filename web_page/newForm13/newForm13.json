      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.test && $data.test_batch ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.test.batch = $data.test_batch;",
        "\treturn {",
        "\t\t//map test variable to expected task contract input",
        "\t\ttestInput: {",
        "\t\t\tname: $data.test.name !== undefined ? $data.test.name : null,",
        "\t\t\tbatch: $data.test.batch ? {",
        "\t\t\t\tbatch: $data.test.batch.batch !== undefined ? $data.test.batch.batch : null",
